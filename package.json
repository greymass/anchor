{
  "name": "anchor-beta",
  "productName": "Anchor-BETA",
  "version": "1.0.0-rc4",
  "description": "An EOSIO Light Wallet with simple tools for basic activities and advanced tools for power users.",
  "scripts": {
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --colors",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --colors",
    "bump": "bump package.json package-lock.json app/package.json app/package-lock.json README.md README.*.md shasum.sh app/renderer/assets/locales/**/common.json",
    "dev": "cross-env START_HOT=1 node -r babel-register ./internals/scripts/CheckPortInUse.js && cross-env START_HOT=1 npm run start-renderer-dev",
    "electron-rebuild": "electron-rebuild --parallel --force --types prod,dev,optional --module-dir app",
    "flow": "flow",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "lint-styles": "stylelint app/*.css app/components/*.css --syntax scss",
    "lint-styles-fix": "stylefmt -r app/*.css app/components/*.css",
    "package-mac": "npm run build && electron-builder --publish never",
    "package-linux": "npm run build && electron-builder --linux --publish never",
    "package-win": "npm run build && electron-builder --win --x64 --ia32 --publish never",
    "postinstall": "node -r babel-register internals/scripts/CheckNativeDep.js && npm run flow-typed && electron-builder install-app-deps && node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "prestart": "npm run build",
    "release": "npm run build && electron-builder -mwl",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron --js-flags='--expose-gc' -r babel-register ./app/main/main.dev",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js",
    "test": "jest --runInBand"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "build": {
    "productName": "Anchor-BETA",
    "appId": "com.greymass.anchor-beta",
    "afterSign": "internals/macos/notarize.js",
    "artifactName": "${os}-${name}-${version}-${arch}.${ext}",
    "asar": true,
    "protocols": {
      "name": "eosio",
      "schemes": [
        "eosio"
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "renderer/assets/",
      "renderer/basic/index.html",
      "renderer/main/index.html",
      "renderer/handler/index.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "publish": {
      "provider": "github"
    },
    "dmg": {
      "icon": "app/renderer/assets/icons/mac/icon.icns",
      "sign": false,
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "entitlements": "internals/macos/entitlements.mac.plist",
      "entitlementsInherit": "internals/macos/entitlements.mac.plist",
      "icon": "app/renderer/assets/icons/mac/icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "win": {
      "certificateFile": "aaroncox.p12",
      "icon": "app/renderer/assets/icons/win/icon.ico",
      "publisherName": "Aaron Cox",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "oneClick": false,
      "createDesktopShortcut": "always",
      "createStartMenuShortcut": true,
      "shortcutName": "anchor"
    },
    "linux": {
      "category": "Network",
      "description": "An EOSIO Light Wallet with simple tools for basic activities and advanced tools for power users.",
      "desktop": {
        "Name": "anchor",
        "GenericName": "Anchor - An EOSIO Light Wallet",
        "X-GNOME-FullName": "anchor",
        "Comment": "An EOSIO Light Wallet with simple tools for basic activities and advanced tools for power users.",
        "Type": "Application",
        "Terminal": "false",
        "StartupNotify": "false",
        "Categories": "Network;"
      },
      "icon": "app/renderer/assets/icons/png",
      "target": [
        {
          "target": "deb",
          "arch": [
            "armv7l",
            "arm64",
            "x64"
          ]
        },
        "AppImage"
      ]
    },
    "snap": {
      "synopsis": "An EOSIO Light Wallet with simple tools for basic activities and advanced tools for power users.",
      "confinement": "devmode",
      "grade": "devel",
      "buildPackages": [
        "gcc",
        "libusb-1.0-0-dev"
      ],
      "stagePackages": [
        "default",
        "libusb-1.0-0"
      ],
      "plugs": [
        "default",
        "hardware-observe",
        "raw-usb"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greymass/eos-voter.git"
  },
  "author": {
    "name": "Greymass",
    "email": "hello@greymass.com",
    "url": "https://github.com/greymass"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greymass/eos-voter/issues"
  },
  "keywords": [
    "anchor",
    "eosio",
    "wallet"
  ],
  "homepage": "https://github.com/greymass/eos-voter#readme",
