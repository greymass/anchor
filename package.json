  "homepage": "https://github.com/greymass/anchor#readme",
  "scripts": {
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js",
    "bump": "bump package.json app/package.json README.md shasum.sh app/renderer/assets/locales/**/common.json",
    "dev": "cross-env START_HOT=1 node -r @babel/register ./internals/scripts/CheckPortInUse.js && cross-env START_HOT=1 npm run start-renderer-dev",
    "electron-rebuild": "electron-rebuild --parallel --force --types prod,dev,optional --module-dir app",
    "flow": "flow",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "lint-styles": "stylelint app/*.css app/components/*.css --syntax scss",
    "lint-styles-fix": "stylefmt -r app/*.css app/components/*.css",
    "package-mac": "npm run build && electron-builder --publish never -c internals/electron-builder.json",
    "package-mas": "npm run build && electron-builder --publish never -c internals/electron-builder.mas.json",
    "package-linux": "npm run build && electron-builder --linux --publish never -c internals/electron-builder.json",
    "package-win": "npm run build && electron-builder --win --x64 --ia32 --publish never -c internals/electron-builder.json",
    "postinstall": "node -r @babel/register internals/scripts/CheckNativeDep.js && npm run flow-typed && electron-builder install-app-deps && node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "prestart": "npm run build",
    "release": "npm run build && electron-builder -mwl",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron --trace-warnings --js-flags='--expose-gc' -r @babel/register ./app/main/main.dev",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack serve --config webpack.config.renderer.dev.js",
    "test": "jest --runInBand"
  },
